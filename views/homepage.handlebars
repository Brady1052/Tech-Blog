<div>
{{#each blogInfo as |blog| }} 
<div class="card">
  <div class="card-header">
    {{ title }}
  </div>
  <div class="card-body">
    <blockquote class="blockquote mb-0">
      <p>{{ blogContent }}</p>
      <footer class="blockquote-footer">{{ username }}</footer>
    </blockquote>
  </div>
</div>
{{/each}} 
</div>